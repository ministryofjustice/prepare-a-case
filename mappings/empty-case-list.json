{
  "request": {
    "method": "GET",
    "urlPathPattern": "/court/([A-Z]*)/cases",
    "queryParameters": {
      "date": {
        "matches": "(\\d{8})"
      }
    }
  },
  "response": {
    "headers": {
      "Content-Type": "application/json",
      "transformers": ["response-template"]
    },
    "status": 200,
    "jsonBody": {
      "cases": []
    }
  }
}
