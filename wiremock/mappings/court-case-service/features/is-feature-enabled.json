{
  "request": {
    "method": "POST",
    "urlPath": "/feature-flags/evaluate",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$[?(@.flagKey == 'prepare-a-case-v2')]"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "enabled": true
    }
  }
}
